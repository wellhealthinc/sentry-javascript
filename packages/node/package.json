{
  "name": "@sentry/node",
  "version": "6.2.3",
  "description": "Official Sentry SDK for Node.js",
  "repository": "git://github.com/getsentry/sentry-javascript.git",
  "homepage": "https://github.com/getsentry/sentry-javascript/tree/master/packages/node",
  "author": "Sentry",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=6"
  },
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sentry/core": "6.2.3",
    "@sentry/eventbuilder-node": "7.0.0-alpha.0",
    "@sentry/integration-common-inboundfilters": "7.0.0-alpha.0",
    "@sentry/integration-node-breadcrumbs": "7.0.0-alpha.0",
    "@sentry/integration-node-contextlines": "7.0.0-alpha.0",
    "@sentry/integration-node-globalhandlers": "7.0.0-alpha.0",
    "@sentry/integration-node-linkederrors": "7.0.0-alpha.0",
    "@sentry/minimal": "6.2.1",
    "@sentry/scope": "7.0.0-alpha.0",
    "@sentry/tracing": "6.2.3",
    "@sentry/transport-base": "7.0.0-alpha.0",
    "@sentry/transport-http": "7.0.0-alpha.0",
    "@sentry/types": "6.2.3",
    "@sentry/utils": "6.2.3",
    "cookie": "^0.4.1",
    "https-proxy-agent": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie": "0.3.2",
    "@types/express": "^4.17.2",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "~10.17.0",
    "express": "^4.17.1",
    "nock": "^13.0.5"
  },
  "scripts": {
    "build": "run-p build:es5 build:esm",
    "build:es5": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:watch": "run-p build:watch:es5 build:watch:esm",
    "build:watch:es5": "tsc -p tsconfig.build.json -w --preserveWatchOutput",
    "build:watch:esm": "tsc -p tsconfig.esm.json -w --preserveWatchOutput",
    "clean": "rimraf dist coverage",
    "link:yarn": "yarn link",
    "lint": "run-s lint:prettier lint:eslint",
    "lint:prettier": "prettier --check \"{src,test}/**/*.ts\"",
    "lint:eslint": "eslint .",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "fix:eslint": "eslint . --fix",
    "test": "run-s test:jest test:express test:webpack",
    "test:jest": "jest -c ../../jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:express": "node test/manual/express-scope-separation/start.js",
    "test:webpack": "cd test/manual/webpack-domain/ && yarn && node npm-build.js",
    "pack": "npm pack"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
